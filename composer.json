{
    "name": "nazemi/laraserve",
    "description": "Create a basic reservation system for Laravel applications.",
    "type": "library",
    "keywords": [
        "php",
        "laravel",
        "reservation",
        "booking",
        "appointment"
    ],
    "authors": [
        {
            "name": "MohamadAli Nazemi",
            "email": "mohamadalinzm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^9.5",
        "phpunit/phpunit": "^11.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nazemi\\Laraserve\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nazemi\\Laraserve\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nazemi\\Laraserve\\LaraserveServiceProvider"
            ]
        }
    }
}
